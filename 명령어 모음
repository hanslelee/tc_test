
# rnn 학습 명령어
python train.py --model_fn ./model_results/rnn_model.pth --train_fn ./data/train_corpus_shuffled.tsv --batch_size=128 --n_epochs 10 --verbose 1 --rnn --gpu_id 0 > ./logs/rnn_train_log.txt

# rnn 추론 명령어
python classify.py --model_fn ./model_results/rnn_model.pth --gpu_id 0 > ./logs/rnn_test_log.txt
# python classify.py --model_fn ./model_results/rnn_model.pth --gpu_id 0 | tee ./logs/rnn_test_log.txt

# rnn 모델 정확도
python ./model_results/get_accuracy.py ./data/test_answer.tsv ./output/rnn_test_output.txt >> ./logs/rnn_result_accuracy.txt






# cnn 학습 명령어
python train.py --model_fn ./model_results/cnn_model.pth --train_fn ./data/train_corpus_shuffled.tsv --batch_size=128 --n_epochs 10 --verbose 1 --cnn --gpu_id 0 > ./logs/cnn_train_log.txt

# cnn 추론 명령어
python classify.py --model_fn ./model_results/cnn_model.pth --gpu_id 0 > ./logs/cnn_test_log.txt

# cnn 모델 정확도
python ./model_results/get_accuracy.py ./data/test_answer.tsv ./output/cnn_test_output.txt >> ./logs/cnn_result_accuracy.txt